{
  "name": "@webcomponents/custom-element-manifest-tools",
  "version": "0.0.0",
  "private": true,
  "description": "Tools for working with Custom Element Manifests",
  "author": "Google LLC",
  "homepage": "",
  "license": "BSD-3-Clause",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "wireit",
    "test": "wireit"
  },
  "wireit": {
    "build": {
      "command": "../../node_modules/.bin/tsc",
      "files": [
        "tsconfig.json",
        "src/**/*.ts"
      ],
      "output": [
        "lib/",
        "tsconfig.tsbuildinfo"
      ],
      "clean": "if-file-deleted"
    },
    "test": {
      "command": "uvu test \".*_test\\.js$\"",
      "dependencies": [
        "build"
      ],
      "files": [],
      "output": []
    }
  },
  "devDependencies": {
    "uvu": "^0.5.6"
  },
  "dependencies": {
    "custom-elements-manifest": "^1.0.0"
  }
}
